    "paths": {
        "/api/v1/hiking-routes/region/{regione_code}/{sda}": {
            "get": {
                "tags": [
                    "hiking-routes"
                ],
                "operationId": "2be1181d637f69dcaf6bb16da6e24d27",
                "parameters": [
                    {
                        "name": "regione_code",
                        "in": "path",
                        "description": "Regione code (e.g. 'l' for tuscany)",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "format": "varchar"
                        }
                    },
                    {
                        "name": "sda",
                        "in": "path",
                        "description": "Number of SDA 'stato di accatastamento' (e.g. 3 or 3,1 or 0,1,2)",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "format": "varchar"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Returns all the hiking routes IDs base on the given region code and SDA number."
                    }
                }
            }
        },
