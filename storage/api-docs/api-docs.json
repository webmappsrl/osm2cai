            "get": {
                "tags": [
                    "hiking-route"
                ],
                "operationId": "11427b4f47709888e728c843ba71736e",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "The ID of a specific Hiking Route (e.g. 2421)",
                        "required": true,
                        "schema": {
